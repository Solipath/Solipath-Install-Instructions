{
	"downloads": [
		{
			"url": "https://downloads.haskell.org/~cabal/cabal-install-${version}/cabal-install-${version}-x86_64-linux-deb10.tar.xz",
			"destination_directory": "${version}",
			"platform_filters": [{"os": "linux", "arch": "x86_64"}]
		},
		{
			"url": "https://downloads.haskell.org/~cabal/cabal-install-${version}/cabal-install-${version}-aarch64-linux-deb10.tar.xz",
			"destination_directory": "${version}",
			"platform_filters": [{"os": "linux", "arch": "aarch64"}]
		},
		{
			"url": "https://downloads.haskell.org/~cabal/cabal-install-${version}/cabal-install-${version}-x86_64-windows.zip",
			"destination_directory": "${version}",
			"platform_filters": [{"os": "windows", "arch": "x86_64"}]
		},
		{
			"url": "https://downloads.haskell.org/~cabal/cabal-install-${version}/cabal-install-${version}-x86_64-darwin.tar.xz",
			"destination_directory": "${version}",
			"platform_filters": [{"os": "macos", "arch": "x86_64"}]
		},
		{
			"url": "https://downloads.haskell.org/~cabal/cabal-install-${version}/cabal-install-${version}-aarch64-darwin.tar.xz",
			"destination_directory": "${version}",
			"platform_filters": [{"os": "macos", "arch": "aarch64"}]
		}
	],
	"environment_variables": [
		{"name": "PATH", "relative_path": "${version}"}
	]
}
